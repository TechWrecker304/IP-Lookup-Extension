{
  "manifest_version": 2,
  "name": "IP Lookup",
  "version": "1.0",
  "description": "Lookup IP addresses on centralops.net and iplocation.com",
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "results.html",
    "results.js"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
